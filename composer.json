{
    "name": "aivus/test-helper",
    "description": "Helper package for testing",
    "minimum-stability": "dev",
    "license": "MIT",
    "keywords": [
        "unit",
        "testing",
        "helper"
    ],
    "authors": [
        {
            "name": "Ilya Antipenko",
            "email": "ilya@antipenko.pp.ua"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.*",
        "satooshi/php-coveralls": "~0.6@stable"
    },
    "autoload": {
        "psr-4": {
            "Aivus\\TestHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aivus\\TestHelper\\Tests\\": "tests/"
        }
    }
}